RewriteBase /
RewriteEngine on
RewriteCond $1 !^(index\.php|flash|css|img|imgs|icon|captcha|images|lib|less|assets|font|statics|js|sound|data|upload|docs|robots\.txt|favicon\.ico|test\.php)
RewriteRule ^(.*)$ index.php/$1 [L]
AddType application/vnd.ms-fontobject .eot
AddType font/ttf .ttf
AddType font/otf .otf
AddType application/font-woff .woff  